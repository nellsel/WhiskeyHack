<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.whiskey.rest.CaskHistory" table="caskhistory" catalog="mydb">
		<id name="CaskHistoryId" type="java.lang.Integer">
			<column name="CaskHistoryId" />
			<generator class="identity" />
		</id>
		<property name="CaskId" column="CaskId" type="int" />
		<property name="DistilleryId" column="DistilleryId" type="int" />
		<property name="AlcoholType" column="AlcoholType" type="string" />
		<property name="CaskHistoryDate" column="CaskHistoryDate" type="string" />
	</class>
</hibernate-mapping>